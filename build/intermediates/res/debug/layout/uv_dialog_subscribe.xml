<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="10dp"
    android:paddingLeft="10dp"
    android:paddingRight="10dp"
    android:paddingTop="10dp" >

    <EditText
        android:id="@+id/uv_email"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:maxLength="254"
        android:hint="@string/uv_email_address"
        android:inputType="textEmailAddress" >

        <requestFocus />
    </EditText>

    <EditText
        android:id="@+id/uv_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:maxLength="25"
        android:hint="@string/uv_name_hint"
        android:inputType="textCapWords" >
    </EditText>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="3dp"
        android:paddingBottom="10dp"
        android:paddingTop="10dp"
        android:text="@string/uv_msg_subscribe"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textColor="#999999" />

</LinearLayout><!-- From: file:/Users/SemonCat/Desktop/gcm-client/userFeedbackSDK/src/main/res/layout/uv_dialog_subscribe.xml --><!-- From: file:/Users/SemonCat/Desktop/gcm-client/GcmClient/build/intermediates/exploded-aar/gcm-client/userFeedbackSDK/unspecified/res/layout/uv_dialog_subscribe.xml -->